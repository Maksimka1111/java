[32m+[m[32mpackage pack;[m
[32m+[m
[32m+[m[32mimport java.io.IOException;[m
[32m+[m[32mimport java.util.Scanner;[m
[32m+[m
[32m+[m[32mpublic class AuthorTester {[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m
[32m+[m[32m    {[m
[32m+[m[32m        Scanner s = new Scanner(System.in);[m
[32m+[m[32m        System.out.print("Name: ");[m
[32m+[m[32m        String name = s.next();[m
[32m+[m[32m        System.out.print("Email: ");[m
[32m+[m[32m        String email = s.next();[m
[32m+[m[32m        char gender;[m
[32m+[m[32m        System.out.print("Gender: ");[m
[32m+[m[32m        gender = (char)System.in.read();[m
[32m+[m[32m        int choice;[m
[32m+[m[32m        Author a = new Author(name,email,gender);[m
[32m+[m[32m        while (true) {[m
[32m+[m[32m            System.out.println("What you wanna to do: ");[m
[32m+[m[32m            System.out.println("1) Get name: ");[m
[32m+[m[32m            System.out.println("2) Get email: ");[m
[32m+[m[32m            System.out.println("3) Set email: ");[m
[32m+[m[32m            System.out.println("4) Get gender: ");[m
[32m+[m[32m            choice = s.nextInt();[m
[32m+[m[32m            if (choice == 1)[m
[32m+[m[32m                System.out.println(a.getName());[m
[32m+[m[32m            if (choice == 2)[m
[32m+[m[32m                System.out.println(a.getEmail());[m
[32m+[m[32m            if (choice == 3) {[m
[32m+[m[32m                System.out.print("Set email: ");[m
[32m+[m[32m                email = s.next();[m
[32m+[m[32m                a.setEmail(email);[m
[32m+[m[32m            }[m
[32m+[m[32m            if (choice == 4)[m
[32m+[m[32m                System.out.println(a.getGender());[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[1mdiff --git a/src/pack10/HowMany.java b/src/pack10/HowMany.java[m
[1mnew file mode 100644[m
[1mindex 0000000..90762a4[m
[1m--- /dev/null[m
[1m+++ b/src/pack10/HowMany.java[m
[36m@@ -0,0 +1,34 @@[m
[32m+[m[32mpackage pack10;[m
[32m+[m
[32m+[m[32mimport java.util.Scanner;[m
[32m+[m
[32m+[m[32mpublic class HowMany {[m
[32m+[m[32m    public static void main(String[] args) {[m
[32m+[m[32m        Scanner s = new Scanner(System.in);[m
[32m+[m[32m        char[] words = new char[1024];[m
[32m+[m[32m        String w;[m
[32m+[m[32m        w = s.nextLine();[m
[32m+[m[32m        words = w.toCharArray();[m
[32m+[m[32m        String alf = "qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM";[m
[32m+[m[32m        char[] alphabet = alf.toCharArray();[m
[32m+[m[32m        int sum = 0;[m
[32m+[m[32m        boolean x = true;[m
[32m+[m[32m        for(int i = 0; i<w.length(); i++)[m
[32m+[m[32m        {[m
[32m+[m[32m            for(int j = 0; j<alf.length();j++)[m
[32m+[m[32m                if (x & words[i] ==alphabet[j])[m
[32m+[m[32m                {[m
[32m+[m[32m                    sum++;[m
[32m+[m[32m                    x = false;[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m[32m                else if (words[i] == alphabet[j])[m
[32m+[m[32m                    break;[m
[32m+[m[32m                else if (words[i]!=alphabet[j] & j == alf.length()-1)[m
[32m+[m[32m                {[m
[32m+[m[32m                    x = true;[m
[32m+[m[32m                }[m
[32m+[m[32m        }[m
[32m+[m[32m        System.out.println("Count of words: " + sum);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
