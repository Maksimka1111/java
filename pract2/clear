[32m+[m[32mpackage pack2;[m
[32m+[m
[32m+[m[32mpublic class Ball {[m
[32m+[m[32m    private double x = 0;[m
[32m+[m[32m    private double y = 0;[m
[32m+[m
[32m+[m[32m    public Ball(double X, double Y) {[m
[32m+[m[32m        x = X;[m
[32m+[m[32m        y = Y;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public double getX() {[m
[32m+[m[32m        return this.x;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setX(double X) {[m
[32m+[m[32m        this.x = X;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public double getY() {[m
[32m+[m[32m        return this.y;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setY(double Y) {[m
[32m+[m[32m        this.y = Y;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setXY(double X, double Y)[m
[32m+[m[32m    {[m
[32m+[m[32m        this.x = X;[m
[32m+[m[32m        this.y = Y;[m
[32m+[m[32m    }[m
[32m+[m[32m    public void move(double xDisp, double yDisp)[m
[32m+[m[32m    {[m
[32m+[m[32m        this.x = this.x + xDisp;[m
[32m+[m[32m        this.y = this.y + yDisp;[m
[32m+[m[32m    }[m
[32m+[m[32m    public String toString()[m
[32m+[m[32m    {[m
[32m+[m[32m        return this.x+" " +this.y;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
